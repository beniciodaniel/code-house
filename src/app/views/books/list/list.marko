<html>
  <head>
      <meta charset="utf-8">
  </head>
  <body>
      <h1> Books List </h1>
      <table>
        <thead>
          <tr>
            <th>ID</th>
            <th>Title</th>
          </tr>
      </thead>
      <tbody>
        <tr for(book in data.books)>
          <td>${book.id}</td>
          <td>${book.title}</td>
        </tr>
      </tbody>

      </table>

  </body> 
</html>